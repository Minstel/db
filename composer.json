    "keywords": ["database", "db"],
    "license": "MIT",
    "homepage": "http://jasny.github.com/db",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
